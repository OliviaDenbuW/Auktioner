@model Nackowskiiiii.Models.AuctionViewModels.AuctionViewModel
@{
    ViewData["Title"] = "ClosedAuction";
}

<h2>ClosedAuction</h2>

<div class="panel panel-default">
    <!-- Default panel contents -->
    <div class="panel-heading">SEARCH RESULT</div><!-- Table -->

    <table class="table">
        <tr>
            <th>Title</th>
            <th>Description</th>
            <th>Start price</th>
            <th>End date</th>
        </tr>

        <tr>
            <td><a asp-controller="Auction" asp-action="ViewAuctionDetails" asp-route-auctionId="2864">@Model.Title</a></td>
            <td>@Model.Description</td>
            <td>@Model.StartPrice</td>
            <td>@Model.EndDateString</td>
        </tr>
        

    </table>
</div>